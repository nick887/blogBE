<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--命名空间为对应的dao/mapper接口-->
<mapper namespace="com.nick.blog.mapper.TimeRecordMapper">

    <select id="getTimeRecord" resultType="com.nick.blog.po.TimeRecord">
        select * from time_record
        limit 0 , 1
    </select>
</mapper>